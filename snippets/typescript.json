{
  "Console Log": {
    "prefix": "clg",
    "body": ["console.log(${0});"],
    "description": "console log"
  },
  "Function": {
    "prefix": "func",
    "body": ["function ${1:function_name}(${2}) {", "	${0:${VISUAL}}", "}"],
    "description": "function"
  },
  "Async Function": {
    "prefix": "funca",
    "body": [
      "async function ${1:function_name}(${2}) {",
      "	${0:${VISUAL}}",
      "}"
    ],
    "description": "async function"
  },
  "Arrow Function": {
    "prefix": "af",
    "body": ["const ${1:function_name} = (${2}) => {", "	${0:${VISUAL}}", "};"],
    "description": "arrow function"
  },
  "Async Arrow Function": {
    "prefix": "aaf",
    "body": [
      "const ${1:function_name} = async (${2}) => {",
      "	${0:${VISUAL}}",
      "};"
    ],
    "description": "async arrow function"
  }
}
